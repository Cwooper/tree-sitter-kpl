{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "kpl",
      "camelcase": "KPL",
      "title": "Kernel Programming Language",
      "scope": "source.kpl",
      "file-types": [
        "k",
        "kpl",
        "h"
      ],
      "injection-regex": "^kpl$",
      "class-name": "TreeSitterKpl"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Kernel Programming Language",
    "authors": [
      {
        "name": "Cwooper",
        "email": "cwooperm@gmail.com",
        "url": "cwooper.me"
      }
    ],
    "links": {
      "repository": "github.com/Cwooper/tree-sitter-kpl",
      "funding": ""
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "java": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}